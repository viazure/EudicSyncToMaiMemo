{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Eudic": {
    "Authorization": "NIS XXX",
    "DefaultBookId": 0
  },
  "MaiMemo": {
    "username": "your_username",
    "password": "your_password",
    "DefaultNotepadId": 0
  }
}
